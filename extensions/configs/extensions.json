{
  "mcp": {
    "servers": {},
    "management": {
      "autoSync": true,
      "updateStrategy": "submodule",
      "versionControl": {
        "enforceExternalRepos": true,
        "allowLocalOverrides": false
      }
    }
  },
  "prompts": {
    "customInstructions": {
      "enabled": true,
      "path": "../prompts/custom-instructions.md"
    }
  },
  "settings": {
    "core": {
      "updateStrategy": "submodule",
      "version": "latest"
    }
  }
}
